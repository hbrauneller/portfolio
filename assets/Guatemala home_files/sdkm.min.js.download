                callbackStart();
            }

            // Send prints only if is Manual or Automatic and not instance mode. Using double NOT for clarity
            var sendPrints = model.get('isManualSelect') || (!model.get('isManualSelect') && model.get('mode') != 'instance') || forceSendPrints;

