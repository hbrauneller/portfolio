                try {
                    scrollTop = Number.isNaN(Number(scrollTop)) ? 0 : scrollTop;
                } catch(e) {
                    //support for IE and other browsers which doesnot support Number.isNan
                    scrollTop =  (typeof(Number(scrollTop)) === 'number' && isNaN(Number(scrollTop))) ? 0 : scrollTop;
