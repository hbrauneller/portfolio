                        var currentOffset = parseInt(stylesContainer.css('transform').indexOf('matrix3d') > 0 ? 0 : stylesContainer.css('transform').replace('matrix(','').replace(')','').split(",")[4].trim());
                        var nextOffset = currentOffset + styleContainerMainWidth;
