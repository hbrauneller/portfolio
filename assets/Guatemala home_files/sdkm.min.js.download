                productMinHeight = Math.min(productWidth, 350);
            }
            else if(numOfProductsInLine == 4){
                productMinHeight = productWidth / productMinRatio;
            }
            else if(numOfProductsInLine == 2){
                productMinHeight = productWidth / productMaxRatio;
