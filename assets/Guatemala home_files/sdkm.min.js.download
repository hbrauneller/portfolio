                var commonWidth         = that.LV_PROD_WIDTH;
                var commonHeight        = that.LV_PROD_HEIGHT;
                var spreadResult        = that.prepareSpreads(products, 'listview', products.length);
                var spreads             = spreadResult.spreads;
