                    var hover = $(view_common.renderTemplate(templates.fullview_prod_hover_tl, {forcedTitleText: btnTitle, productType: product['type'], clickthroughNoModal: that.clickthroughNoModal }));
                    $(container).append(hover);

                    return top;
